#include<stdio.h>
void towers(int n,char src,char destn,char aux)
{
    if(n==1)
    {
        printf("MOVE DISK 1 FROM PEG %c TO PEG %c\n",src,destn);
    }
    else
    {
       towers(n-1,src,aux,destn);
       printf("MOVE DISK %d from PEG %c TO PEG %c\n",n,src,destn);
       towers(n-1,aux,destn,src);

    }
}
int main()
{
    int n;
    printf("Enter the no of disks\n");
    scanf("%d",&n);
    towers(n,'A','C','B');
}
